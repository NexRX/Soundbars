{
  "$schema": "https://github.com/tauri-apps/tauri/raw/dev/tooling/cli/schema.json",
  "productName": "stickynow",
  "version": "0.0.0",
  "identifier": "uk.co.nexhub.stickynow",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "StickyNow",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
